(function(){	var maskObj:MovieClip = new MovieClip();	maskObj.graphics.beginFill(0x00000000);		var mX = 317;	var mY = 165;	tbi.noSkullB = addImage("noskullblurred", mX-12, mY);							tbi.noSkullB.mask = maskObj;		toolbox.bringForward();		var pSwab = addImage("bonewaxArm", 0, stage.height);		toolbox.bringForward();			var mousePos = {x:WIDTH, y:HEIGHT};		var cleanup = function(){		remove(pSwab);	}		var getMousePos = function(){		mousePos.x = mouseX;		mousePos.y = mouseY;	}	var drawTimerFunc = function(){		var tweenX = null;		var tweenY = null;		return function(){			var mousex = mousePos.x;			var mousey = mousePos.y;			if(tweenX != null){				tweenX.stop();			}			tweenX = createTween(pSwab, "x", None.easeNone, mousex-15);						if(tweenY != null){				tweenY.stop();			}			tweenY = createTween(pSwab, "y", None.easeNone, mousey-30);							var swabPos ={"x" : mouseX, "y":mouseY};						var rad = 10;						//need to update these graphics based o			maskObj.graphics.drawCircle(pSwab.x+15, pSwab.y+30, rad);			tbi.noSkullB.mask = maskObj;		}	}		var checkDrawFunc = function(callback){		return function(){			var checkPoints = new Array({x: 313, y: 261},{x: 344, y: 194},{x: 418, y: 175},{x: 475, y: 187},{x: 528, y: 198},{x: 551, y: 251},{x: 566, y: 284},{x: 530, y: 317},{x: 496, y: 358},{x: 438, y: 355},{x: 395, y: 347},{x: 365, y: 336},{x: 337, y: 303},{x: 330, y: 281},{x: 376, y: 176})			var misses = 0;			for(var i in checkPoints){				var point:Point = new Point(checkPoints[i].x, checkPoints[i].y);				if (!tbi.noSkullB.hitTestPoint(point.x, point.y, true)){					misses++;					if(misses>1){						return;					}				}			}			callback();		}		}		var removeArm = function(callback){		var tweens = new Array();		var waiter = null;				maskObj.graphics.drawCircle(WIDTH/2, HEIGHT/2, WIDTH);		tbi.noSkullB.mask = maskObj;				tweens.push(createTween(pSwab, "y", None.easeInOut, HEIGHT, -1, 100, function(){			waiter.kill();			callback();		}));				waiter = waitOnUser(function(){			for(var i in tweens){				tweens[i].fforward();				tweens[i] = null;			}			tweens = null;		});			}		var mousex=0;	var mousey=0;		stage.addEventListener(MouseEvent.MOUSE_MOVE, getMousePos);	var shaver = timer(25, drawTimerFunc(), 0);	var checkDraw = timer(500, checkDrawFunc(function(){						stage.removeEventListener(MouseEvent.MOUSE_MOVE, getMousePos);			shaver.stop(); 			shaver=null;			checkDraw.stop(); 			checkDraw = null;			removeArm(function(){				cleanup();				//hack to get it to redo the surgerypatient code				lastFrame = -1;				gotoAndStop("Scene_SurgeryPatient");			});	}), 0);	})();