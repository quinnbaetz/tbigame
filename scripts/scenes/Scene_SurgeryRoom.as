(function(){	 var playerDialog = function(callback){		return displayMessages(_('no tools'), 50, 360, callback, true);			}	var surgeonDialog = function(callback){		return displayMessages(_('doctor needs to talk'), null, null, callback, false, "surgeonFace");	}	var surgeonDialogTools = function(callback){		return displayMessages(_('collect tools'), null, null, callback, false, "surgeonFace");	}	var surgeonDialogIncision = function(callback){		return displayMessages(_('first incision'), null, null, callback, false, "surgeonFace");	}			var removeMsg = function(playerMsg){		if(playerMsg !== null && typeof(playerMsg.remove) === "function"){			playerMsg.remove();			playerMsg = null;		}		return null;	}	var msg = null;	var hairHead = addImage('zoomed_out_hair_head', 287, 269);	switch(timeline){		case 100:									fadeIn(function(){				var playerMsg = null;				var surgeonMsg = null;												var SurgeonButton:ClickRegion = new ClickRegion(stage, 0, 0, 150, 500, function(){					remove(hairHead);					playerMsg = removeMsg(playerMsg);					surgeonMsg = removeMsg(surgeonMsg);					trayButton.remove();					patientButton.remove();					SurgeonButton.remove();					gotoAndStop("Scene_SurgeonFace");									});												var patientButton:ClickRegion = new ClickRegion(stage, 150, 200, 350, 300, function(){					removeMsg(playerMsg);					removeMsg(surgeonMsg);					playerMsg = playerDialog();				});								var trayButton:ClickRegion = new ClickRegion(stage, 600, 300, 200, 300, function(){					removeMsg(playerMsg);					removeMsg(surgeonMsg);					surgeonMsg = surgeonDialog();				});			});		break;		case 102:			msg = surgeonDialogTools();				case 108: /*Hack because 108 has the same sequence as 102*/			if(msg === null){				msg = surgeonDialogIncision();				remove(hairHead);				hairHead = null;			}			var patientButton:ClickRegion = new ClickRegion(stage, 150, 200, 350, 300, function(){				removeMsg(msg);				msg = playerDialog();			});						var trayButton:ClickRegion = new ClickRegion(stage, 600, 300, 200, 300, function(){				if(hairHead !== null){					remove(hairHead);				}				msg = removeMsg(msg);				trayButton.remove();				patientButton.remove();				timeline--; /*hack to get timeline back on track */				gotoAndStop("Scene_SurgeryTray");			});		break;		case 109:			remove(hairHead);			hairHead = null;		case 103:			var patientButton:ClickRegion = new ClickRegion(stage, 150, 200, 350, 300, function(){				if(hairHead !== null){					remove(hairHead);				}				patientButton.remove();				timeline--; /*hack to get timeline back on track */				gotoAndStop("Scene_SurgeryPatient");			});		break;			}})();