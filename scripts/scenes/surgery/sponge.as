import flash.display.MovieClip;import flash.geom.Point;(function(){ 	var mX = 350;	var mY = 183;	 	var pSpongeArm = new patchArm();	pSpongeArm.y = HEIGHT;	stage.addChild(pSpongeArm);		pSpongeArm.gotoAndStop(0);		toolbox.bringForward();		var sugeonDialog = function(callback){		var msgs = displayMessages(_('sponge success'), null, null, callback, false, "surgeonFace");	}		var tweenX = null;	var tweenY = null;	var mousePos = {x: mouseX, y: mouseY};	var getMousePos = function(){		mousePos.x = mouseX;		mousePos.y = mouseY;	}		var finished = function(){		pSpongeArm.gotoAndStop(2);		var duraOpenH =  addImage("brainSponged", mX-55, mY-69);		bringToFront(stage, pSpongeArm);		toolbox.bringForward();		if(clipHandMove){			clipHandMove.stop();			trace("hand stop moving");		}		clipHandMove = null;				stage.removeEventListener(MouseEvent.MOUSE_MOVE,getMousePos);		tweenY = createTween(pSpongeArm, "y", None.easeInOut, HEIGHT, -1, 100);		sugeonDialog(function(){			fadeOut(function(){										ExternalInterface.call("callToActionscript", 19, 50);										remove(duraOpenH);					remove(tbi.rclips);					remove(tbi.muscleback);					remove(tbi.duraOpenH);					/*gotoAndStop(2);					addImage("sepaLogo", WIDTH/2-200, HEIGHT/2+100);					addImage("neuronLogo",  WIDTH/2+100, HEIGHT/2+100);					var textMsg:TextField = new TextField();					textMsg.textColor = "0xFFFFFF"; 					textMsg.text = "To Be Continued...";					textMsg.x = HEIGHT/2-textMsg.textWidth/2;					textMsg.y += WIDTH/2-textMsg.textHeight/2-300;					textMsg.width = 150;					textMsg.height = 100;					addChild(textMsg);*/													}); 		});	};		stage.addEventListener(MouseEvent.CLICK, function(){			if(mousePos.x > 472 && mousePos.x < 545 && mousePos.y > 224 && mousePos.y < 345){				stage.removeEventListener(MouseEvent.CLICK,arguments.callee);				finished();			}else{				//display error			}						      						   						   	});	stage.addEventListener(MouseEvent.MOUSE_MOVE, getMousePos);	var clipHandMove = timer(25, function(){				var mousex = mousePos.x;		var mousey = mousePos.y;		if(tweenX != null){			tweenX.stop();		}		if(tweenY != null){			tweenY.stop();		}			var deltaX = 950;		var deltaY = 270;				tweenX = createTween(pSpongeArm, "x", None.easeInOut, mousex-deltaX);		tweenY = createTween(pSpongeArm, "y", None.easeInOut, mousey-deltaY);		//maskObject.graphics.drawCircle(pSpongeArm.x+deltaX, pSpongeArm.y+deltaY, 10);   			}, 0);					})();